{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "braininfryingpan/multi-container-client",
            "hostname": "client",
            "essestial": false,
            "memory": 128

        },
        {
            "name": "server",
            "image": "braininfryingpan/multi-container-server",
            "hostname": "api",
            "essestial": false,
            "memory": 128
            
        },
        {
            "name": "worker",
            "image": "braininfryingpan/multi-container-worker",
            "hostname": "worker",
            "essestial": false,
            "memory": 128
            
        },
        {
            "name": "nginx",
            "image": "braininfryingpan/multi-container-nginx",
            "hostname": "nginx",
            "essestial": false,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client","server"],
            "memory": 128
            
        }
    ]
}